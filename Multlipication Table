#include<stdio.h>

 main()
 {
        int i,num,ans;
        printf("Enter a number to display its multiplication table !!!\n");
        scanf("%d",&num);

        for(i=1;i<=10;i++)
            {
            ans=1;
            ans=i*num;
            printf("%d X %d = %d\n",num,i,ans);
            }

 }
